{
  "name": "@baseloop/dev",
  "version": "1.2.8",
  "description": "Development tools such as baseloop-dev-server.",
  "bin": {
    "baseloop": "dist/bin/baseloop.js"
  },
  "type": "module",
  "main": "dist/es2015/index.js",
  "module": "dist/es2015/index.js",
  "es2015": "dist/es2015/index.js",
  "types": "dist/es2015/index.d.ts",
  "typings": "dist/es2015/index.d.ts",
  "files": [
    "/dist",
    "/README.md",
    "/example-project"
  ],
  "scripts": {
    "test": "jest",
    "build": "rm -rf dist && npm run build:es2015 && npm run build:bin",
    "build:es2015": "tsc --module es2015 --target es2015 --outDir dist/es2015",
    "build:bin": "tsc --build tsconfig-bin.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint ./ --ext .js,.jsx,.ts,.tsx",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test",
    "tsc": "tsc"
  },
  "keywords": [
    "baseloop",
    "reactive",
    "functional",
    "rxjs",
    "react",
    "unidirectional",
    "web"
  ],
  "author": "Kai Sellgren <kaisellgren@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "chalk": "^2.4.2",
    "express": "^4.16.4",
    "fs-extra": "^7.0.1",
    "ws": "^6.2.0",
    "yargs": "^13.2.2",
    "webpack": ">=4.0.0 <5.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/express": "^4.16.1",
    "@types/ws": "^6.0.3",
    "@types/fs-extra": "^7.0.0",
    "@types/node": "^12.7.2",
    "@types/webpack": "^4.39.1",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-jest": "^24.4.0",
    "eslint": "^5.14.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "jest": "^24.4.0",
    "prettier": "^1.18.2",
    "typescript": "^3.4.4",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
